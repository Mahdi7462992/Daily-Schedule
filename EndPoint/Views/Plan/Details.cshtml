@model List<Plan>

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

    <div>
        <h4>Person</h4>
        <hr />
    @for (int i = 0; i < Model.Count; i++)
    {
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model[i].Detail)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model[i].Detail)
            </dd>
        </dl>
    }
    </div>
    
    <div>
        <a asp-action="Index">Back to List</a>
    </div>

